<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>shop</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <class>info.magnolia.module.shop.ShopModule</class>
  <versionHandler>info.magnolia.module.shop.setup.ShopModuleVersionHandler</versionHandler>
  <version>${project.version}</version>
  
  <dependencies>
    <dependency>
      <name>core</name>
      <version>4.2.3/*</version>
    </dependency>
    <dependency>
      <name>data</name>
      <version>1.6.0/*</version>
    </dependency>
    <dependency>
      <name>standard-templating-kit</name>
      <version>1.4.2/*</version>
    </dependency>
    <dependency>
      <name>demo-project</name>
      <version>1.4.2/*</version>
    </dependency>
    <dependency>
      <name>theme-pop</name>
      <version>1.4.2/*</version>
    </dependency>
    <dependency>
      <name>form</name>
      <version>1.3/*</version>
    </dependency>
    <dependency>
      <name>categorization</name>
      <version>1.1/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>ocm</name>
      <version>1.0/*</version>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <name>magnolia</name>
      <workspaces>
        <workspace>shoppingCarts</workspace>
      </workspaces>
    </repository>
  </repositories>
</module>
