<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>shop</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <class>info.magnolia.module.shop.ShopModule</class>
  <versionHandler>info.magnolia.module.shop.setup.ShopModuleVersionHandler</versionHandler>
  <version>${project.version}</version>
  <components>
      <id>main</id>
    <component>
      <type>info.magnolia.module.shop.util.SimpleBean2ContentProcessorImpl</type>
      <implementation>info.magnolia.module.shop.util.SimpleBean2ContentProcessorImpl</implementation>
      <scope>singleton</scope>
      <lazy>true</lazy>
  </component>
</components>
  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-framework</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>standard-templating-kit</name>
      <version>2.9/*</version>
    </dependency>
    <dependency>
      <name>demo-project</name>
      <version>2.9/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>theme-pop</name>
      <version>2.9/*</version>
    </dependency>
    <dependency>
      <name>dam</name>
      <version>2.1/*</version>
    </dependency>
    <dependency>
      <name>resources</name>
      <version>2.4/*</version>
    </dependency>
    <dependency>
      <name>form</name>
      <version>2.3/*</version>
    </dependency>
    <dependency>
      <name>categorization</name>
      <version>2.4/*</version>
    </dependency>
    <dependency>
      <name>ocm</name>
      <version>1.3/*</version>
    </dependency>
    <dependency>
      <name>rssaggregator</name>
      <version>2.4</version>
    </dependency>
    <dependency>
      <name>site</name>
      <version>1.0/*</version>
    </dependency>
  </dependencies>

  <repositories>
    <repository>
      <name>magnolia</name>
      <workspaces>
        <workspace>shoppingCarts</workspace>
        <workspace>shops</workspace>
        <workspace>shopProducts</workspace>
        <workspace>shopSuppliers</workspace>
      </workspaces>
      <nodeTypeFile>/mgnl-nodetypes/magnolia-module-shop-nodetypes.xml</nodeTypeFile>
    </repository>
  </repositories>
</module>
